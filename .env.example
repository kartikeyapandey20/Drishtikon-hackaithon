# Database configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/visually_impaired_app

# FastAPI configuration
API_V1_STR=/api/v1
PROJECT_NAME=Visually Impaired Assistant API
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:8000", "http://localhost:8080"]

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=60
ALGORITHM=HS256

# Vision Model settings
# Options: openai, google, huggingface
VISION_MODEL_PROVIDER=openai 
VISION_MODEL_NAME=gpt-4-vision-preview
OPENAI_API_KEY=your-openai-api-key-here
GOOGLE_API_KEY=your-google-api-key-here
HUGGINGFACE_API_KEY=your-huggingface-api-key-here

# LLM settings
# Options: openai, google, huggingface
LLM_PROVIDER=openai
LLM_MODEL_NAME=gpt-4-turbo
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=1000

# Storage configuration
# Options: local, gcs, s3
STORAGE_PROVIDER=local
LOCAL_STORAGE_PATH=./storage
GCS_BUCKET_NAME=your-gcs-bucket-name
GCS_CREDENTIALS_FILE=path-to-gcs-credentials.json
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=your-aws-region
AWS_BUCKET_NAME=your-aws-bucket-name

# Logging
LOG_LEVEL=INFO

# Rate limiting
RATE_LIMIT_PER_MINUTE=100 